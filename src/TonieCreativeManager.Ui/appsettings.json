{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "TonieBoxCreativeManager": {
    "MediaFileExtensions": [ ".mp3", ".m4a", ".wav", ".ogg", ".wma" ],
    "FolderCoverFiles": [ "cover.jpg", "cover.png", "cover.gif", "front.jpg", "front.png", "folder.jpg", "folder.png" ],
    "IgnoreFolderNames": [ ".@__thumb" ],
    "MarkFolderAsHiddenFile": ".hidden",
    "RepositoryDataFile": "creative-tonie-manager.json",
    "EnableShop": true,
    "KeyboardCharacters": [ "👻", "🐵", "🐱", "🐷", "🦄", "🐢", "🐬", "🐧", "🐻", "🐘" ],
    "MarkFolderAsBoughtFile": ".bought",
    "MediaItemBuyCost": 3,
    "MediaItemUploadCost": 1
  }
}
